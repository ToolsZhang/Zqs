{
  "name": "zqs-core",
  "version": "0.0.7",
  "description": "A simple restful server framework using node and typescript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf lib && rimraf coverage",
    "watch:test": "jest --watch",
    "unit:test": "jest ./__tests__/app-spec.ts ",
    "prepare": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "docs": "gulp typedoc --plugin typedoc-plugin-markdown",
    "build": "tsc --pretty",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ToolsZhang/zqs-core.git"
  },
  "keywords": [
    "node.js"
  ],
  "author": "Zq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ToolsZhang/zqs-core/issues"
  },
  "homepage": "https://github.com/ToolsZhang/zqs-core#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@koa/cors": "^3.1.0",
    "@types/colors": "^1.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.155",
    "@types/mongoose": "^5.7.24",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^8.10.61",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-redis": "^1.0.26",
    "@types/spdy": "^3.4.4",
    "@types/xml2json": "^0.11.0",
    "bytes": "^3.1.0",
    "chalk": "^4.1.0",
    "colors": "^1.4.0",
    "crypto": "^1.0.1",
    "ejs": "^3.1.3",
    "humanize-number": "^0.0.2",
    "js2xml": "^1.0.9",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.12.1",
    "koa-better-body": "^3.3.9",
    "koa-compose": "^4.1.0",
    "koa-compress": "^4.0.1",
    "koa-conditional-get": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "mongoose": "~5.9.18",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.3",
    "passthrough-counter": "^1.0.0",
    "qs": "^6.9.4",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.3.0",
    "spdy": "^4.0.2",
    "typed-promisify": "^0.4.0",
    "xml2json": "^0.12.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "canvas": "^2.6.1",
    "coveralls": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^2.2.5",
    "jest": "^25.0.0",
    "jest-environment-node-debug": "^2.0.0",
    "mock-require": "^3.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "~0.15.0",
    "typedoc-plugin-markdown": "~2.1.0",
    "typescript": "^3.9.5",
    "zqs-plugin-bookmark": "^1.0.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*.spec|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
